{
    "name": "paskuale75/yii2-anagrafica",
    "description": "All data for manage name, surname, city, address, etc...",
    "type": "yii2-extension",
    "license": "MIT",
    "authors": [
        {
            "name": "Pasquale Pellicani",
            "email": "pellicanipasquale@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0.0",
        "yiisoft/yii2": "~2.0.14",
        "yiisoft/yii2-bootstrap4": "^2.0",
        "kartik-v/yii2-krajee-base": "@dev",
        "paskuale75/yii2-comuni-italiani": "@dev",
        "davidepastore/codice-fiscale":"@dev"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "type": "vcs",
            "url": "https://github.com/paskuale75/comuni-italiani"
        }
    ],
    "autoload": {
        "psr-4": {
            "paskuale75\\anagrafica\\": ""
        }
    }
}